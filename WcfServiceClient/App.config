<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
      <bindings>
        <basicHttpBinding>
          <binding name="BasicHttpBinding_ITestService" />
        </basicHttpBinding>
        <netTcpBinding>
          <binding name="NetTcpBinding_ITestService">
            <security mode="None" />
          </binding>
        </netTcpBinding>
      </bindings>
      <client>
        <endpoint address="net.tcp://localhost:9000" binding="netTcpBinding"
            bindingConfiguration="NetTcpBinding_ITestService" contract="ITestService"
            name="NetTcpBinding_ITestService" />
      </client>
    </system.serviceModel>
</configuration>